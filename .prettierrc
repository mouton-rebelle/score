{
  "printWidth": 80,
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "tabWidth": 2,
        "parser": "scss"
      }
    },
    {
      "files": "*.yml",
      "options": {
        "parser": "yaml",
        "tabWidth": 4
      }
    },
    {
      "files": "*.yaml",
      "options": {
        "parser": "yaml",
        "tabWidth": 4
      }
    }
  ]
}
